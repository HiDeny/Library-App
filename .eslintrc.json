{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:import/recommended",
        "airbnb-base",
        "prettier"

    ],
    "plugins": ["import"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
        
    },
    "rules": {
        "import/extensions": ["error", "always", {"js": "always"}]

    }
}
